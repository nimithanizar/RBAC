  "scripts": {
    "start": "npm run env:dev && craco start",
    "start-prod": "npm run env:prod && craco start",
    "start-staging": "npm run env:staging && craco start",
    "start-live": "npm run env:live && craco start",
    "build": "npm run env:dev && craco --max_old_space_size=4096 build",
    "build-prod": "npm run env:prod && craco --max_old_space_size=4096 build",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,json,md,html,css,less,scss}\"",
    "test": "craco test",
    "eject": "craco eject",
    "env:dev": "node set-environment.js dev",
    "env:prod": "node set-environment.js production",
    "release": "standard-version",
    "deploy-dev": "npm run build-prod && scp -r ./build/* 35.240.147.20:/apps/admin.rbac.staging",
    "deploy-staging": "npm run build-staging && scp -r ./build/* 35.240.147.20:/apps/admin.rbac.staging",
    "gcloud-deploy-live": "npm run build-live && cp app-live.yaml build/app.yaml && cd build && gcloud config set project rbac-admin-live && gcloud app deploy --project=mediaone-admin-live --verbosity=debug --quiet && npm run gcloud-deploy-live-log",
    "gcloud-deploy-live-log": "gcloud app logs tail -s default --project=rbac-admin-live"
  },
